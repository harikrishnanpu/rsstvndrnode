<p id="userName" style="display: none;">{{user.name}}</p>
<p id="userPhone" style="display: none;">{{user.phone}}</p>

<script>
  let uname = document.getElementById('userName').innerText;
  let uphone = document.getElementById('userPhone').innerText;
  localStorage.setItem("logged", "true")
  localStorage.setItem("userName", uname);
  localStorage.setItem("userPhone", uphone);
</script>



